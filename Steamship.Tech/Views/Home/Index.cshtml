@model IEnumerable<Steamship.Tech.Models.EmployeeDetail>

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-left">
    <h1 class="display-4">Welcome</h1>
    @foreach (var employee in Model)
    {
        <p>Employee Name: @employee.EmployeeName</p>
        <p>Line Manager Name: @employee.LineManagerName</p>
        <p>Total Employee Sales: @employee.TotalSales</p>
        <p>Total Employee Sales Figure: @employee.TotalSalesFigure</p>
    }
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>
